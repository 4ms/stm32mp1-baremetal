#include "r8a77990-ebisu.dts"
#include "r8a77990-u-boot.dtsi"

&pfc {
	pinctrl-0 = <&scif_clk_pins>;
	pinctrl-names = "default";

	scif_clk_pins: scif_clk {
		groups = "scif_clk_a";
		function = "scif_clk";
	};
};

&sdhi0 {
	sd-uhs-sdr12;
	sd-uhs-sdr25;
	max-frequency = <208000000>;
};

&sdhi1 {
	sd-uhs-sdr12;
	sd-uhs-sdr25;
	max-frequency = <208000000>;
};

&vcc_sdhi0 {
	u-boot,off-on-delay-us = <20000>;
};

&vcc_sdhi1 {
	u-boot,off-on-delay-us = <20000>;
};
