#include "r8a77965-m3nulcb.dts"
#include "r8a77965-u-boot.dtsi"

/ {
	cpld {
		compatible = "renesas,ulcb-cpld";
		status = "okay";
		gpio-sck = <&gpio6 8 0>;
		gpio-mosi = <&gpio6 7 0>;
		gpio-miso = <&gpio6 10 0>;
		gpio-sstbz = <&gpio2 3 0>;
	};
};

&sdhi0 {
	sd-uhs-sdr12;
	sd-uhs-sdr25;
	sd-uhs-sdr104;
	max-frequency = <208000000>;
	status = "okay";
};

&sdhi2 {
	mmc-hs400-1_8v;
	max-frequency = <200000000>;
	status = "okay";
};

&vcc_sdhi0 {
	u-boot,off-on-delay-us = <20000>;
};
