/include/ "fsl-ls1012a.dtsi"

/ {
	model = "LS1012A RDB Board";
	aliases {
		spi0 = &qspi;
	};
};

&esdhc1 {
	mmc-hs200-1_8v;
};

&qspi {
	bus-num = <0>;
	status = "okay";

	qflash0: s25fl128s@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "jedec,spi-nor";
		spi-max-frequency = <20000000>;
		reg = <0>;
	};
};

&i2c0 {
	status = "okay";
};

&duart0 {
	status = "okay";
};

&sata {
	status = "okay";
};
