/ {
	chosen {
		u-boot,dm-pre-reloc;
	};

	ahb {
		u-boot,dm-pre-reloc;

		apb {
			u-boot,dm-pre-reloc;

			pinctrl {
				u-boot,dm-pre-reloc;
			};
		};
	};
};

